{
  "presets": [
    ["env", {
      "targets": {
        "electron": "1.8.2"
      }
    }],
    "react"
  ],
  "plugins": [
    ["transform-runtime", {"polyfill": false}],
    "transform-class-properties",
    "transform-object-rest-spread"
  ],
  "env": {
    "production": {
      "presets": [
        ["es2015", {"modules": false}],
        "react"
      ],
      "plugins": [
        ["transform-runtime", {"polyfill": false}],
        "transform-class-properties",
        "transform-object-rest-spread"
      ]
    }
  }
}