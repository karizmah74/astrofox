{
  "name": "Astrofox",
  "version": "1.0.0-beta.1",
  "main": "app/main.js",
  "productName": "Astrofox",
  "description": "Astrofox",
  "author": "Mike Cao <mike@mikecao.com>",
  "homepage": "http://astrofox.io",
  "private": true,
  "scripts": {
    "start": "electron ./app",
    "build-win": "build --x64 --win",
    "build-mac": "build --x64 --mac"
  },
  "build": {
    "appId": "io.astrofox.astrofox",
    "productName": "Astrofox",
    "asar": true,
    "extraResources": [
      {
        "from": "resources/ffmpeg/${os}/${arch}",
        "to": "bin"
      }
    ],
    "mac": {
      "category": "public.app-category.video"
    },
    "win": {
      "target": [
        "squirrel"
      ]
    },
    "linux": {
      "category": "AudioVideo"
    },
    "squirrelWindows": {
      "iconUrl": "http://astrofox.io/icon.ico"
    }
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "id3js": "^1.1.3",
    "immutable": "^3.8.1",
    "mime": "^1.3.4",
    "node-rsa": "^0.4.2",
    "react": "^15.4.1",
    "react-addons-css-transition-group": "^15.4.1",
    "react-dom": "^15.4.1",
    "three": "^0.83.0",
    "tinycolor2": "^1.4.1"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-babili": "^0.0.9",
    "babel-preset-es2015-node6": "^0.4.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^7.3.0",
    "babili": "^0.0.9",
    "browserify": "^13.1.1",
    "bundle-collapser": "^1.2.1",
    "electron": "^1.4.13",
    "electron-builder": "^10.15.1",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^2.3.2",
    "gulp-duration": "0.0.0",
    "gulp-glsl": "^1.0.2",
    "gulp-iconfont": "^8.0.1",
    "gulp-less": "^3.3.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.2.0",
    "gulp-template": "^4.0.0",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.8",
    "loose-envify": "^1.3.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.8.0"
  }
}
