{
  "name": "Astrofox",
  "version": "1.0.0-beta.5",
  "main": "app/main.js",
  "productName": "Astrofox",
  "description": "Astrofox",
  "author": "Mike Cao <mike@mikecao.com>",
  "homepage": "http://astrofox.io",
  "private": true,
  "scripts": {
    "run": "electron ./app",
    "dev": "webpack --watch --config webpack.config.all.js",
    "build-prod": "webpack -p --config webpack.config.all.js",
    "build-win": "electron-builder --x64 --win",
    "build-mac": "electron-builder --x64 --mac",
    "lint": "eslint --ext .js,.json,.jsx src/js || exit 0"
  },
  "build": {
    "appId": "io.astrofox.astrofox",
    "productName": "Astrofox",
    "asar": true,
    "files": [
      "**/*",
      "!**/*.map",
      "!*.yml",
      "!*.lock"
    ],
    "extraResources": [
      {
        "from": "resources/ffmpeg/${os}/${arch}",
        "to": "bin"
      }
    ],
    "mac": {
      "category": "public.app-category.video"
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "category": "AudioVideo"
    },
    "nsis": {
      "oneClick": true
    },
    "squirrelWindows": {
      "iconUrl": "https://astrofox.io/icon.ico"
    },
    "publish": {
      "provider": "generic",
      "url": "https://astrofox.io/download/${os}",
      "channel": "latest"
    }
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "debug": "^3.0.1",
    "del": "^3.0.0",
    "id3js": "^1.1.3",
    "immutable": "^3.8.1",
    "mime": "^2.0.2",
    "node-rsa": "^0.4.2",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-transition-group": "^2.2.0",
    "three": "^0.87.1",
    "tinycolor2": "^1.4.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-minify": "^0.2.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "electron": "^1.7.6",
    "electron-builder": "^19.27.7",
    "electron-updater": "^2.8.9",
    "eslint": "^4.7.2",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "glsl-man": "^1.1.12",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "style-loader": "^0.18.2",
    "svg-sprite-loader": "^3.2.5",
    "url-loader": "^0.5.9",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2"
  }
}
